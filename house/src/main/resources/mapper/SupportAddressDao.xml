<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dzj.house.dao.SupportAddressDao">

	<select id="getCity" resultType="com.dzj.house.entity.SupportAddress">
		select * from support_address where level = #{level} order by belong_to asc
	</select>
	
	<select id="getRegion" resultType="com.dzj.house.entity.SupportAddress">
		select * from support_address where level=#{level} and belong_to=#{belongTo}
	</select>
</mapper>
